@prefix acl: <http://www.w3.org/ns/auth/acl#>.
@prefix  foaf:  <http://xmlns.com/foaf/0.1/>.
<#owner>
  a acl:Authorization;
  acl:agent <http://jackson.localhost:8080/profile/card#me>;
  acl:accessTo <./>;
  acl:default <./>;
  acl:mode
    acl:Read, acl:Write, acl:Control.
<#public>
  a acl:Authorization;
  acl:agent foaf:Agent;
  acl:accessTo <./>;
  acl:default <./>;
  acl:mode
    acl:Append.